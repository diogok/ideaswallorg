<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
	<application>ideaswall</application>
	<version>6</version>
	<!-- Configure java.util.logging -->
	<system-properties>
		<property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
	</system-properties>
	<!-- For PHP-Support -->
	<static-files>
		<exclude path="/*.php" />
	</static-files>
	<resource-files>
		<include path="/**.php" />
	</resource-files>
</appengine-web-app>
